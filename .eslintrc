{
  "extends": "airbnb-base",
  "env": {
    "node": true
  },
  "rules": {
    "import/no-extraneous-dependencies": [
    "error",
    {
      "devDependencies": false,
      "optionalDependencies": false,
      "peerDependencies": false,
      "no-underscore-dangle": ["error", { "allow": ["_id"] }],
    "no-console": "off",
    "linebreak-style": 0
    }
  ]
  }
}
